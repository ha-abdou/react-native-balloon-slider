{
  "expo": {
    "name": "react native balloon slider",
    "slug": "react-native-balloon-slider",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Balloon slider",
    "githubUrl": "https://github.com/ha-abdou/react-native-balloon-slider"
  }
}
